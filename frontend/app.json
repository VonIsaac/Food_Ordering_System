{
  "expo": {
    "name": "tsokolatea",
    "slug": "ai-driven_food-ordering",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/tsokolatea.jpg",
    "scheme": "aidrivenfoodordering",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundImage": "./assets/images/mtea.jpg"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/mtea.jpg",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
